kind: "BuildConfig"
apiVersion: "v1"
metadata:
  name: "test-build" 
spec:
  runPolicy: "Serial" 
  source: 
    git:
      uri: "https://github.com/rhopenshift-online/origin-test"
  strategy:
    sourceStrategy:
      from:
        kind: "ImageStreamTag"
        name: "mybusybox:latest"
  output: 
    to:
      kind: "ImageStreamTag"
      name: "busybox:latest"

